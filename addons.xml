<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.vietmediaF" name="VietmediaF" version="11.35" provider-name="ducnn">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.six"/>
        <import addon="script.module.requests"/>
        <import addon="script.module.beautifulsoup4"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="autorun.py"/>
    <extension point="xbmc.addon.metadata">
        <reuselanguageinvoker>true</reuselanguageinvoker>
        <summary lang="en">VietmediaF for Kodi</summary>
        <description lang="en">
            VietmediaF
        </description>
        <disclaimer lang="en">Nội dung lấy trên mạng và do người dùng tự upload lên. VietmediaF không chịu trách nhiệm về nội dung hiển thị bên trong addon.
        </disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
</addon>

<addon id="plugin.program.openwizard" name="OpenWizard" version="2.0.7" provider-name="openwizard">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
        <import addon="script.kodi.android.update" optional="true" version="1.1.7" />
        <import addon="script.speedtester" optional="true" version="1.1.2" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">OpenWizard</summary>
        <description lang="en">OpenWizard - Công cụ bảo trì và quản lý Kodi.</description>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
</addon>

<addon id="repo_vietmediaf_offical" name="VIETMEDIAF_OFFICAL" version="0.0.1" provider-name="repo_vietmediaf_offical">
    <extension point="xbmc.addon.repository" name="repo_vietmediaf_offical">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/kodivmf/vietmediaf_offical/main/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/kodivmf/vietmediaf_offical/main/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/kodivmf/vietmediaf_offical/main/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>repo_vietmediaf_offical</summary>
        <description>Repository cho VIETMEDIAF_OFFICAL</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.skinhelper" name="SkinHelper" version="0.0.4" provider-name="heppen">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.pil"/>
    </requires>
    <extension point="xbmc.python.script" library="default.py">
        <provides>executable</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Simple Skin Helper</summary>
        <description lang="en_GB">This script is made by heppen to help skinning</description>
        <language></language>
        <platform>all</platform>
    </extension>
</addon>

<addon id="skin.arctic.zephyr.2.resurrection.mod" name="Arctic: Zephyr 2 - Resurrection" provider-name="jurialmunkey, pkscout, heppen" version="1.0.36">
    <requires>
        <import addon="xbmc.gui" version="5.17.0" />
        <import addon="script.skinshortcuts"/>
        <import addon="script.skinhelper" version="0.0.4" />
        <import addon="script.skinvariables" version="2.1.14" />
        <import addon="plugin.video.themoviedb.helper" version="5.4.8" />
        <import addon="resource.images.studios.white"/>
        <import addon="resource.images.moviegenreicons.transparent"/>
        <import addon="resource.images.weathericons.white"/>
        <import addon="resource.images.moviecountryicons.maps"/>
    </requires>
    <extension point="xbmc.gui.skin" debugging="false">
        <res width="1920" height="1080" aspect="16:9" default="true" folder="1080i" />
    </extension>
    <extension point="xbmc.addon.metadata">
        <news></news>
        <summary lang="en">Clean, minimal, elegant, simple, light.</summary>
        <summary lang="vi">Gọn gàng, tối giản, thanh lịch, đơn giản, nhẹ nhàng.</summary>
        <description lang="en">This is a light mod of Arctic Zephyr 2 by heppen. Arctic Zephyr 2 is a minimalistic skin created by jurialmunkey and moded to work with Kodi 19 Matrix by pkscout. The skin is designed to be simple so that the user can focus on their media content. In addition to the default light colour scheme, the skin also includes a number of darker variations.</description>
        <platform>all</platform>
        <license>Creative Commons Attribution Non-Commercial Share-Alike 4.0</license>
        <forum>https://forum.kodi.tv/showthread.php?tid=362941</forum>
        <source>https://github.com/DenDyGH/skin.arctic.zephyr.2.resurrection.mod</source>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
            <screenshot>resources/screenshot-01.jpg</screenshot>
            <screenshot>resources/screenshot-02.jpg</screenshot>
            <screenshot>resources/screenshot-03.jpg</screenshot>
            <screenshot>resources/screenshot-04.jpg</screenshot>
            <screenshot>resources/screenshot-05.jpg</screenshot>
            <screenshot>resources/screenshot-06.jpg</screenshot>
        </assets>
    </extension>
</addon>
</addons>
